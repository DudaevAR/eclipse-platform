TEST STEPS

1. Press Create New Folder button in existeng Project
2. Set invalid name of folder (*, /, \, |, ?, :, ")
3. Make sure that error is displayed in "New Folder" window
------=_.description-216f885c-d591-38ce-8ea2-e4f8cb4d6ffa--
------=_.content-0a7243a0-75d3-3d5f-9791-539de0e5b7ac
Content-Type: text/ecl
Entry-Name: .content

emit {
   set-param symbol "*"
   set-param message "* is an invalid character in resource name \'*\'."
} {
   set-param symbol "?"
   set-param message "? is an invalid character in resource name \'?\'."
} {
   set-param symbol "|"
   set-param message "| is an invalid character in resource name \'|\'."
} {
   set-param symbol "/"
   set-param message "Invalid path for resource \'/MyProject\'. Must include project and resource name."
} {
   set-param symbol "\\"
   set-param message "Invalid path for resource \'/MyProject\'. Must include project and resource name."
} {
   set-param symbol ":"
   set-param message "Invalid path for resource \'/MyProject\'. Must include project and resource name."
} {
   set-param symbol "\""
   set-param message "\" is an invalid character in resource name \'\"\'."
} {
   set-param symbol " "
   set-param message "\' \' is an invalid name on this platform."
} | repeat-with {
	get-view "Project Explorer" | get-tree | select MyProject | get-menu "New/Folder" | click
	get-window "New Folder" | get-editbox -after [get-label "Folder name:"] | set-text [get-param symbol]
	with [get-window "New Folder"] {
	    get-editbox -after [get-label Folder] | get-property text | equals [get-param message] | verify-true
	    get-button Finish | get-property enablement | equals false | verify-true
	}
	get-window "New Folder" | get-button Cancel | click
}
------=_.content-0a7243a0-75d3-3d5f-9791-539de0e5b7ac--
